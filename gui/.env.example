# SR Matcher GUI Environment Variables
# Copy to .env.local for local development

# API Origin (sr-api base URL)
VITE_API_ORIGIN=http://localhost:3000

# Authentication (Phase 1: API Key, Phase 2: JWT)
VITE_API_KEY=your-api-key-here
# Optional: JWT for AUTH_MODE=jwt (takes precedence over API key)
VITE_JWT_TOKEN=
